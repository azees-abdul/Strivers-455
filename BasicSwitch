//Switch Use case

class Solution{
    static double switchCase(int choice, List<Double> arr){
        // code here
        Switch(choice){
        case 1:
            double r = Math.PI*arr.get(0)*arr.get(0);
            return r;
        case 2:
            double area = arr.get(0)*arr.get(1);
            return area;
    }
    }
}
